    "import tkinter as tk\n",
    "from tkinter import ttk\n",
    "\n",
    "win = tk.Tk()\n",
    "win.title(\"Tab 101\")\n",
    "\n",
    "tab_control = ttk.Notebook(win)\n",
    "\n",
    "tab1 = ttk.Frame(tab_control)\n",
    "tab_control.add(tab1, text='Tab1')\n",
    "tab_control.pack(expand=1, fill=\"both\")"
   ]
  },
